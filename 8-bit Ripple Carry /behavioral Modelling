module rca8(A, B, Cin, Sum, Cout);
  input  [7:0] A, B;
  input        Cin;
  output reg [7:0] Sum;
  output reg       Cout;

  always @(*) begin
    {Cout, Sum} = A + B + Cin;
// other way  temp = A + B + Cin;    Perform addition
//    Sum  = temp[7:0];      lower 8 bits are sum
 //   Cout = temp[8];        MSB is carry out    
  end
endmodule

